{
  "version": 2,
  "builds": [
    {
      "src": "frontend/react-app/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    { "src": "api/main.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/", "dest": "/" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
